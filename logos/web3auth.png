    <script>
      var editor = {
        wallet: ace.edit("editor-wallet"),
        direct: ace.edit("editor-direct"),
        tkey: ace.edit("editor-tkey"),
      };
      var walletInput = {
        wallet: [
          'import OpenLogin from "@toruslabs/openlogin"',
          "\n\n",
          "const sdk = new OpenLogin()",
          "\n",
          "sdk.init()",
          "\n",
          "sdk.login()",
          "\n",
          "console.log(sdk.privKey)",
          "\n",
        ],
        direct: [
          'import OpenLogin from "@toruslabs/openlogin"',
          "\n\n",
          "const sdk = new OpenLogin()",
          "\n",
          "sdk.init()",
          "\n",
          "sdk.login()",
          "\n",
          "console.log(sdk.privKey)",
          "\n",
        ],
        tkey: [
          'import OpenLogin from "@toruslabs/openlogin"',
          "\n\n",
          "const sdk = new OpenLogin()",
          "\n",
          "sdk.init()",
          "\n",
          "sdk.login()",
          "\n",
          "console.log(sdk.privKey)",
          "\n",
        ],
      };
      var timeouts = [];
      // Setup Editors
      setupEditor(editor.wallet);
      setupEditor(editor.direct);
      setupEditor(editor.tkey);
      // listen to tab changes
      document.querySelectorAll(".editor-selector").forEach(function (elem) {
        elem.addEventListener("click", function () {
          var showType = this.dataset.type;
          if (!this.classList.contains("w--current")) {
            timeouts.forEach((timeout) => clearTimeout(timeout));
            addTexts(editor[showType], walletInput[showType]);
          }
        });
      });
      // listen to scroll
      window.addEventListener("scroll", triggerEditorOnScroll);
      function triggerEditorOnScroll() {
        var elementTarget = document.getElementById("torus-feats");
        if (
          window.scrollY > elementTarget.offsetTop - elementTarget.offsetHeight + 100 &&
          window.scrollY < elementTarget.offsetTop + elementTarget.offsetHeight - 600
        ) {
          var currentSelector = document.querySelector(".editor-selector.w--current");
          addTexts(editor[currentSelector.dataset.type], walletInput[currentSelector.dataset.type]);
          window.removeEventListener("scroll", triggerEditorOnScroll);
        }
      }
      function setupEditor(editor) {
        editor.setTheme("ace/theme/tomorrow_night");
        editor.session.setMode("ace/mode/javascript");
        editor.session.setTabSize(2);
        editor.setKeyboardHandler("ace/keyboard/vim");
        editor.setBehavioursEnabled(false);
        editor.setOptions({
          autoScrollEditorIntoView: true,
        });
        editor.session.on("change", () => {
          editor.renderer.scrollToLine(Number.POSITIVE_INFINITY);
        });
      }
      function addTexts(editor, editorInput) {
        var timer = 0;
        editor.setValue("");
        editorInput.forEach((text, i) => {
          var timeout = setTimeout(() => {
            addChars(editor, text, timer);
          }, timer);
          timeouts.push(timeout);
          timer += text.length * 20 + 300;
        });
      }
      function addChars(editor, textLine, timer) {
        var chars = [textLine];
        if (textLine !== "\n") {
          chars = textLine.split("");
        }
        chars.forEach((char, i) => {
          timeout = setTimeout(() => {
            editor.insert(char);
          }, i * 20);
          timeouts.push(timeout);
        });
      }
      new ClipboardJS(".copy-to-clipboard");
    </script>
