    <script type="text/javascript">
      function convertFormToJSON(form) {
        var array = $(form).serializeArray();
        var json = {};
        json["email"] = array[0].value || "";
        return json;
      }
      $('form[action="https://api-auth.web3auth.io/api/v1/web3auth-newsletter"]').each(function (i, el) {
        var form = $(el);
        form.submit(function (e) {
          e.preventDefault();
          form = $(e.target);
          var data = convertFormToJSON(form);
          var action = form.attr("action");
          $.ajax({
            url: action,
            method: "POST",
            beforeSend: function (request) {
              request.setRequestHeader("accept", "application/json");
              request.setRequestHeader("content-type", "application/json");
            },
            data: JSON.stringify(data),
            success: function () {
              var parent = $(form.parent());
              // Hide the form
              parent.children("form").css("display", "none");
              // Display the "Done" block
              parent.children(".w-form-done").css("display", "block");
            },
            error: function (e) {
              if (e.responseJSON.message === "Contact already exist") {
                var parent = $(form.parent());
                // Hide the form
                parent.children("form").css("display", "none");
                // Display the "Done" block
                parent.children(".w-form-done").css("display", "block");
              } else {
                var parent = $(form.parent());
                // Display the "Failed" block
                parent.find(".w-form-fail").css("display", "block");
              }
            },
          });
        });
      });
    </script>
    <script>
      /**
       * Close-either-to-close-both top banners,
       * but reset the state after a page reload.
       */
      (function initializeTopBanners() {
        // ----  configuration  ----
        const BANNERS = [
          { bannerId: "dark-top-banner", closeId: "dark-top-banner-close" },
          { bannerId: "lite-top-banner", closeId: "lite-top-banner-close" },
        ];
        const STORAGE_KEY = "top-banners-closed";
        // -------------------------
        // 1.  Hide immediately if flagged as closed (same page view).
        if (sessionStorage.getItem(STORAGE_KEY) === "true") {
          BANNERS.forEach(({ bannerId }) => {
            const banner = document.getElementById(bannerId);
            if (banner) banner.style.display = "none";
          });
        }
        // 2.  Wire each close button to hide both banners.
        BANNERS.forEach(({ bannerId, closeId }) => {
          const banner = document.getElementById(bannerId);
          const closeBtn = document.getElementById(closeId);
          if (!banner || !closeBtn) return; // skip if markup missing
          closeBtn.addEventListener("click", () => {
            BANNERS.forEach(({ bannerId }) => {
              const b = document.getElementById(bannerId);
              if (b) b.style.display = "none";
            });
            sessionStorage.setItem(STORAGE_KEY, "true");
          });
        });
        // 3.  Clear the “closed” flag right before the page is unloaded
        //     (covers refresh, hard reload, or navigating away).
        window.addEventListener("beforeunload", () => {
          sessionStorage.removeItem(STORAGE_KEY);
        });
      })();
    </script>
  </body>
